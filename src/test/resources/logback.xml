<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="dbAppender"
		class="ch.qos.logback.ext.spring.DelegatingLogbackAppender" />
	<appender name="consoleAppender"
		class="ch.qos.logback.ext.spring.DelegatingLogbackAppender" />
	<root level="INFO">
		<appender-ref ref="dbAppender" />
		<appender-ref ref="consoleAppender" />
	</root>
</configuration>